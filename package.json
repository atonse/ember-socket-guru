    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-sass": "^7.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^2.2",
