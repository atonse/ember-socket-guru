    "ember-addon",
    "web sockets",
    "pusher",
    "socket.io",
    "action cable",
    "phoenix channels",
    "pusher-wrapper",
    "ember-pusher",
    "ember-socketio",
    "ember-phoenix-channels",
    "ember-actioncable",
    "ember"
  ],
  "license": "MIT",
  "author": {
    "name": "Jacek Bandura",
    "email": "jacek.bandura@netguru.pl"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/netguru/ember-socket-guru",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember try:each",
    "deploy": "ember github-pages:commit -m \"Deploy gh-pages from commit $(git rev-parse HEAD)\"; git push origin gh-pages:gh-pages; git checkout master"
  },
